program MJProgram int x; int z[];
{
	int main() int y;{
		z = new int[5];
		z[1] = 3;
		print(z[0]);
		
		print(1, 1);
		
		print('a', 1);
		
		
		
		y = 5;
		
		// x = 8;
		x = y + 3;
		print(x);
		
		// y = 8
		y = (x);
		
		// x = 9
		x++;
		print(x);
		
		// x = 8
		x--;
		print(((x)));
		
		// x = 6
		x = x+(-2);
		print(x);
		
		// x = 16
		x = y*2;
		print(x);
		
		// y = 4
		y = x / 4;
		print(y);
		
		// x = 1
		x = y % 3;
		print(x);
		
		// out = 4*in
		read(x);
		x = x*y;
		print(x);


		print(false, 20);
		
		return 0;
	}
}