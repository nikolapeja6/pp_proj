program MJProgram
{
	
	int main() int i;{	

		i = 10;
		
		do
			{
			
			print(i, 3);
			print(':', 1);
			
			if(i%2 == 0)
				print(2,1);
			//else
			//	print('x',1);
				
			if(i%3 == 0)
				print(3,1);
			//else
			//	print('x',1);
				
			if(i%5 == 0)
				print(5,1);
			//else
			//	print('x',1);
			
			if(i%7 == 0)
				print(7,1);
			//else
			//	print('x',1);
				
			i++;
			
			}
		while (i);
	
		return 0;
	}
}