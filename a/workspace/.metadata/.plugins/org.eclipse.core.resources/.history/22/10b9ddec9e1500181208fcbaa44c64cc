	private Term getTermFromExpr(Expr expr)
	{
		if(expr instanceof PositiveExpr)
		{
			return ((PositiveExpr)expr).getTerm();
		}
		else
		{
			return ((NegativeExpr)expr).getTerm();
		}
	}
	
