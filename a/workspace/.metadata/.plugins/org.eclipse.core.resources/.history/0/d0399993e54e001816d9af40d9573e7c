program BBB

	class A {
		int b;
		{
		void p1(){print('1');}
		void pa(){print('a');}
		}
	}
	
	class B{
		A a;
		int x;
		{
		void p2(){return;}
		}
	}
	
	class C{
		B b;
		{
		void p3(){return;}
		}
	}
{
	void main() C c; A a;
	{
		c = new C;
		
		c.b = new B;
		
		c.b.a = new A;
		
		a = new A;
		a.p1();
		
		print(c.b.a.b);
		print(c.b.x);
		
		c.b.a.b = 5;
		
		print(c.b.a.b);
		print(c.b.x);
		
		c.b.x = 2;
		
		print(c.b.a.b);
		print(c.b.x);
		
		c.b.a.b = 5;
		
		print(c.b.a.b);
		print(c.b.x);
		
		c.b.a.b = c.b.a.b * c.b.a.b * c.b.x;
		
		print(c.b.a.b);
		print(c.b.x);
		
		//a = new A;
		//a.b = 5;
		
		//print(a.b);
	}
}