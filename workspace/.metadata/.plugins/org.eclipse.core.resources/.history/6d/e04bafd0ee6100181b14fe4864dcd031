	}


	void sleep(int time)int i;{
		
		i = 0;
		do
			i++;
		while(i < time);
	
	}
		
	void end(){
		print('T');
		print('h');
		print('e');
		print(' ');
		print('E');
		print('n');
		print('d');
		print(' ');
		print(':');
		print(')');
		
	}

	void main(){
	
		hello();
	
		sleep(SLEEP_TIME);
	
