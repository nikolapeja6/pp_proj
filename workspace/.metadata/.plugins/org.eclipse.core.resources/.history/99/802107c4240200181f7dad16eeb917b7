

/**
 *  TERMINALS: 
 *  
 *  CONTROL FLOW
 *  LITERALS
 *  OPERATORS
 *  	ARITHMETIC
 *  	COMPARISON
 *  	LOGIC
 *  	ASSIGNMENT
 *  	SIDE-EFFECT
 *  
 */

/** CONTROL FLOW TERMINALS */
terminal IF, ELSE, BREAK, DO, WHILE, CONTINUE;


/** LITERALS */
terminal NUMBER, CHAR, STRING;

/** OPERATORS */

/** ARITHMETIC OPERATORS */
terminal PLUS, MINUS, MULTUPLY, DIV, MOD;

/** COMPARISON OPERATORS */
terminal EQUAL, NOT_EQUAL, GREATER, LESS, GREATER_OR_EQUEL, LESS_OR_EQUEL;

/** LOGIC OPERATORS */
terminal AND, OR;

/** ASSIGNMENT */
terminal ASSIGN;

/** SIDE EFFECT */
terminal INC, DEC;



/**
 * END OF TERMINALS
 */

Program ::= PROG IDENT



IDENT

RBRACE
RPAREN

SEMI
RETURN

LPAREN
LBRACE
COMMA
EOF

PROG
PRINT
error
VOID


CLASS
NEW

EXTENDS





SEMICOLON
DOT
OPEN_PARENTHESES
CLOSED_PARENTHESES
OPEN_BRACES
CLOSED_BRACED
OPEN_CHEVRONS
CLOSED_CHEVRONS

BOOL
