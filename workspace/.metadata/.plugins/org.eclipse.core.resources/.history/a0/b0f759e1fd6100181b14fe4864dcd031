program Green1B

const int three = 3;

int x;
bool f,f2,f4;

{

	int two(){
		f2 = true;
		return 2;
	}
	
	
	int four() int res;
	{
		f4 = true;
		res = two() * two();
		return res;
	}
	
	void out(){
		if(f)
			print('1');
		else
			print('0');
		
		if(f2)
			print('1');
		else
			print('0');
		
		if(f4)
			print('1');
		else
			print('0');
		
		
		print(eol);
		
	}
	
	void readNum(bool ret[] )int i;{
		i = 0;
		do{
			read(ret[i]);
			i++;
		}while(i < four());
	}

	int transform(bool num []) int i, res;{
		i = 0;
		res = 0;
		do{
			res = res*2+num[i];
			num[i] = 1;
			i++;
		}while(i < three + 1);
		
		return res;
	}
	
	
	void end(){
		print('T');
		print('h');
		print('e');
		print(' ');
		print('E');
		print('n');
		print('d');
		print(' ');
		print(':');
		print(')');
		
	}
	
	void rec(int x, int y)
	int z;
	{
		print(x,3);
		print(y,3);
		z = x-1;
		print(z,3);
		print(eol);
		if(z > 0){
			rec(z,y);
		}
		else
		{
			print('x');
			print(eol);
		}
		print(x,3);
		print(y,3);
		print(z,3);
		print(eol);
	}

	void main() bool val[];{
		f = false;
		f2 = false;
		f4 = false;
		
		// 000
		out();
		
		// 2
		print(two());
		print(eol);
		
		//020
		out();
		
		f2 = false;
		
		// 4
		print(four());
		print(eol);
		
		//024
		out();
	
		do {
			print('*');
		
			// read value
			val = new bool[four()];
			readNum(val);
			x = transform(val);
			// print actual value
			print(x);
			print(eol);
			
			// 1111
			print(val[0],1);
			print(val[1], 1);
			print(val[2],1);
			print(val[3], 1);
			print(eol);
			
			if(x % 2 == 0)
				print('p');
			else
				print('n');
			
			print(eol);
			
			print(eol);
		}while(x != 9);
		

		rec(5,3);
		
		//  5  3  4
		//  4  3  3
		//  3  3  2
		//  2  3  1
		//  1  3  0
		//x
		//  1  3  0
		//  2  3  1
		//  3  3  2
		//  4  3  3
		//  5  3  4
		
		end();
	
	}

}