program BBB

	class A {
		int b;
	}
	
	class B{
		A a;
	}
	
	class C{
		B b;
	}
{
	void main() C c;
	{
		c = new C;
		
		c.b = new B;
		
		c.b.a = new A;
		
		c.b.a.b = 5;
		
		print(c.b.a.b);
		
		c.b.a.b = c.b.a.b * c.b.a.b;
		
		print(c.b.a.b);
		
		//a = new A;
		//a.b = 5;
		
		//print(a.b);
	}
}