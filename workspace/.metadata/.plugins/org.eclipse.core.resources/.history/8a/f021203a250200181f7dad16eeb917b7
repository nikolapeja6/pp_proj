

/**
 *  TERMINALS: 
 *  
 *  CONTROL FLOW
 *  LITERALS
 *  OPERATORS
 *  	ARITHMETIC
 *  	COMPARISON
 *  	LOGIC
 *  	ASSIGNMENT
 *  	SIDE-EFFECT
 *  PARENTHESES
 *  
 */

/** CONTROL FLOW TERMINALS */
terminal IF, ELSE, BREAK, DO, WHILE, CONTINUE, RETURN;


/** LITERALS */
terminal NUMBER, CHAR, STRING, BOOL;


/** OPERATORS */

/** ARITHMETIC OPERATORS */
terminal PLUS, MINUS, MULTUPLY, DIV, MOD;

/** COMPARISON OPERATORS */
terminal EQUAL, NOT_EQUAL, GREATER, LESS, GREATER_OR_EQUEL, LESS_OR_EQUEL;

/** LOGIC OPERATORS */
terminal AND, OR;

/** ASSIGNMENT */
terminal ASSIGN;

/** SIDE-EFFECT */
terminal INC, DEC;

/** ALLOCATION */
terminal NEW;

/** END OF OPERATORS */


/** PARENTHESES */
terminal	OPEN_PARENTHESES, CLOSED_PARENTHESES,
			OPEN_BRACES, CLOSED_BRACED,
			OPEN_CHEVRONS, CLOSED_CHEVRONS;


/**
 * END OF TERMINALS
 */

Program ::= PROG IDENT



IDENT

SEMI

COMMA
EOF

PROG
PRINT
error
VOID


CLASS


EXTENDS

SEMICOLON
DOT


